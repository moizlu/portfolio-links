---
import '../styles/global.css';

interface Props {
    faviconPath: string;
    title: string;
}
const { faviconPath, title }: Props = Astro.props;
---

<html lang="ja">
	<head prefix="og: https://ogp.me/ns#">
		<meta property="og:url" content="https://moiz.lu/" />
		<meta property="og:type" content="profile" />
		<meta property="og:title" content="もいずる | リンク集" />
		<meta property="og:image" content="https://moiz.lu/ogp.png" />
		<meta property="og:site_name" content="もいずる | リンク集" />
		<meta property="og:description" content="リンク集" />

		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href={faviconPath} />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body class="bg-[#333333] font-primary text-gray-200">
        <slot />
	</body>
</html>

